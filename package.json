{
  "name": "disha-sawant-portfolio",
  "version": "1.0.0",
  "description": "Professional portfolio website for Disha Sawant - Computer Engineering Graduate Student & AI Application Engineer",
  "main": "index.html",
  "scripts": {
    "start": "python -m http.server 8000 || php -S localhost:8000 || npx serve",
    "serve": "npx serve .",
    "dev": "npx live-server --port=8000 --open=index.html",
    "build": "echo 'No build step required for static HTML'",
    "deploy:surge": "npx surge .",
    "deploy:netlify": "npx netlify deploy --prod --dir=.",
    "validate:html": "npx html-validate index.html",
    "validate:css": "npx css-validator styles.css",
    "lighthouse": "npx lighthouse http://localhost:8000 --output=html --output-path=./lighthouse-report.html",
    "minify:css": "npx clean-css-cli -o styles.min.css styles.css",
    "minify:js": "npx uglify-js script.js -o script.min.js",
    "optimize": "npm run minify:css && npm run minify:js"
  },
  "keywords": [
    "portfolio",
    "resume",
    "computer-engineering",
    "ai",
    "machine-learning",
    "full-stack",
    "web-development",
    "responsive",
    "static-site"
  ],
  "author": {
    "name": "Disha Sawant",
    "email": "dishasawantt@gmail.com",
    "url": "https://linkedin.com/in/disha-sawant-7877b21b6/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/portfolio.git"
  },
  "homepage": "https://yourusername.github.io/portfolio",
  "devDependencies": {
    "live-server": "^1.2.2",
    "serve": "^14.0.0",
    "html-validate": "^8.0.0",
    "clean-css-cli": "^5.6.0",
    "uglify-js": "^3.17.0",
    "lighthouse": "^11.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}